<h1>Listing all startup records</h1>

<table>
  <tr>
    <th>Time</th>
    <th>Package</th>
    <th>Package<br/>version</th>
    <th>With image</th>
    <th>Startup<br/>time ms</th>
    <th>Manufacturer</th>
    <th>Model</th>
    <th>Android<br/>version</th>
    <th>Ruboto<br/>platform<br/>version</th>
    <th>Ruboto<br/>app<br/>version</th>
  </tr>

<% @startups.each do |startup| %>
  <% if startup.created_at.to_date != @previous_date %>
  <% @previous_date = startup.created_at.to_date %>
          <tr>
            <td colspan="3" nowrap="true"><%= startup.created_at.strftime '%Y-%m-%d' %></td>
          </tr>
  <% end %>
  <tr>
    <td nowrap="true"><%= startup.created_at.strftime '%H:%M' %></td>
    <td><%= startup.package %></td>
    <td><%= startup.package_version %></td>
    <td><%= startup.with_image %></td>
    <td><%= startup.startup_time %></td>
    <td><%= startup.manufacturer %></td>
    <td nowrap="true"><%= startup.model %></td>
    <td><%= startup.android_version %></td>
    <td><%= startup.ruboto_platform_version %></td>
    <td><%= startup.ruboto_app_version %></td>
  </tr>
<% end %>
</table>
